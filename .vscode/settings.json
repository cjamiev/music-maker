{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.alwaysShowStatus": true,
  "eslint.options": {
    "extensions": [
      ".html",
      ".js",
    ]
  },
  "eslint.validate": [
    "javascript"
  ],
  "files.exclude": {
    "node_modules/": true
  }
}