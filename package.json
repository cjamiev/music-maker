{
  "name": "piano-app",
  "version": "0.4.0",
  "private": true,
  "description": "Create Sheet music and display future songs to play",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack serve --config config/webpack.dev.js",
    "build": "webpack --config config/webpack.prod.js",
    "format": "prettier --write \"src/**/*.js\"",
    "eslint-fix": "eslint --ext .js src/ -f html -o ./coverage/eslint-report.html",
    "coverage": "jest --env=jsdom --coverage --json --outputFile=./coverage/outputfile.json",
    "e2e": "cypress run",
    "e2e:watch": "cypress open",
    "test": "jest --env=jsdom --watch",
    "t": "jest --env=jsdom src/components/atoms/KeySignature/KeySignature.test.js --watch"
  },
  "devDependencies": {
    "@babel/core": "7.15.8",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/preset-env": "7.15.8",
    "@babel/preset-react": "7.14.5",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "7.0.2",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.2.5",
    "babel-loader": "8.2.2",
    "babel-plugin-module-resolver": "4.1.0",
    "css-loader": "6.4.0",
    "cypress": "8.5.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-react": "1.1.7",
    "eslint-loader": "4.0.2",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "file-loader": "6.2.0",
    "html-loader": "2.1.2",
    "html-webpack-plugin": "5.3.2",
    "jest": "27.2.5",
    "prettier": "2.4.1",
    "react-test-renderer": "17.0.2",
    "redux-logger": "3.0.6",
    "style-loader": "3.3.0",
    "webpack": "5.58.1",
    "webpack-bundle-analyzer": "4.4.2",
    "webpack-cli": "4.9.0",
    "webpack-dev-server": "4.3.1"
  },
  "dependencies": {
    "axios": "0.22.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.5",
    "react-router-dom": "5.3.0",
    "redux": "4.1.1",
    "redux-thunk": "2.3.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ],
          "alias": {
            "constants": "./src/constants"
          }
        }
      ]
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "moduleDirectories": [
      "src",
      "src/testHelper",
      "node_modules"
    ],
    "moduleNameMapper": {
      "testHelper(.*)$": "<rootDir>/src/testHelper/$1"
    },
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/cssTransformer.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/fileTransformer.js"
    },
    "testPathIgnorePatterns": [
      "src/testHelper"
    ],
    "testRegex": "src/.*\\.test.(js|jsx)$",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/config/setupTests.js"
    ]
  }
}
